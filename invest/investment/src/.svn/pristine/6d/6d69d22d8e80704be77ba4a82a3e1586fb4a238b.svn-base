package com.gxcz.xuhui.investment.model.dto;

import java.util.List;

import com.gxcz.xuhui.investment.model.BaseModel;

/**
 * 
 * @author devuser5
 * 
 * 返回结果DTO抽取
 * 
 *
 */
public class ResultDTO {
	
	private boolean success;
	private String errCode;
	private String error;
	
	private PagerDTO pagerDTO;
	private BaseModel baseModel;
	private List<?> dataDto;
	 
	
	public BaseModel getBaseModel() {
		return baseModel;
	}
	public void setBaseModel(BaseModel baseModel) {
		this.baseModel = baseModel;
	}
	public List<?> getDataDto() {
		return dataDto;
	}
	public void setDataDto(List<?> dataDto) {
		this.dataDto = dataDto;
	}
	public PagerDTO getPagerDTO() {
		return pagerDTO;
	}
	public void setPagerDTO(PagerDTO pagerDTO) {
		this.pagerDTO = pagerDTO;
	}
	public String getError() {
		return error;
	}
	public void setError(String error) {
		this.error = error;
	}
	public boolean getSuccess() {
		return success;
	}
	public void setSuccess(boolean success) {
		this.success = success;
	}
	public String getErrCode() {
		return errCode;
	}
	public void setErrCode(String errCode) {
		this.errCode = errCode;
	}

	
	
	

}
