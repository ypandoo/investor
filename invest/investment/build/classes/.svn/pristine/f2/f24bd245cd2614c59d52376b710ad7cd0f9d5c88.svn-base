package com.gxcz.xuhui.investment.model;

import java.math.BigDecimal;

public class SubscribeSummaryInfo {
	
	private int projectCount;
	
	private int personCount;
	
	private BigDecimal subscribeAmt;
	
	private BigDecimal bonusAmt;
	
	private BigDecimal contributiveAmt;
	
	private BigDecimal leverageAmt;

	public int getProjectCount() {
		return projectCount;
	}

	public void setProjectCount(int projectCount) {
		this.projectCount = projectCount;
	}

	public int getPersonCount() {
		return personCount;
	}

	public void setPersonCount(int personCount) {
		this.personCount = personCount;
	}


	public BigDecimal getBonusAmt() {
		return bonusAmt;
	}

	public void setBonusAmt(BigDecimal bonusAmt) {
		this.bonusAmt = bonusAmt;
	}

	public BigDecimal getSubscribeAmt() {
		return subscribeAmt;
	}

	public void setSubscribeAmt(BigDecimal subscribeAmt) {
		this.subscribeAmt = subscribeAmt;
	}

	public BigDecimal getLeverageAmt() {
		return leverageAmt;
	}

	public void setLeverageAmt(BigDecimal leverageAmt) {
		this.leverageAmt = leverageAmt;
	}

	public BigDecimal getContributiveAmt() {
		return contributiveAmt;
	}

	public void setContributiveAmt(BigDecimal contributiveAmt) {
		this.contributiveAmt = contributiveAmt;
	}
	
	

}
